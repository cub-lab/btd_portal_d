    </changeSet>
    <changeSet id="2" author="cub">
        <addColumn tableName="CUB_INDIVIDUAL_CLIENT">
            <column name="FIRST_NAME" type="VARCHAR(255)"/>
            <column name="LAST_NAME" type="VARCHAR(255)"/>
        </addColumn>
    </changeSet>
</databaseChangeLog>